package pack;

public class MaqExp {

	public static void main(String[] args) {
		
		Moneda m = new Moneda();
		Producto p = new Producto();
		
		double d;		//dinero acumulado
		double precio;	//precio de cada producto
		
		d = m.pedir();
		precio = p.elegir();
		
		while (d < precio) {
			System.out.println("Dinero insuficiente");
			d = d + m.pedir();
		}
		
	}
}